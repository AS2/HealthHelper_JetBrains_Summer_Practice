<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.user.healthhelper">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Health Helper"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AddProductActivity"
            android:label="Add new product"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".ParametersActivity"
            android:label="Parameters"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".RecomendationsActivity"
            android:label="Suggestions"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".EatenActivity"
            android:label="Today eaten"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".OneRecomendationActivity"
            android:label="Current suggestion"
            android:parentActivityName=".RecomendationsActivity"/>
        <activity
            android:name=".CurrentProductActivity"
            android:label="Product info"
            android:parentActivityName=".AddProductActivity"></activity>
    </application>

</manifest>